<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<!-- 用于测试开始，提交时需注释掉 -->
	<!-- 提供方应用信息，用于计算依赖关系 -->  
    <dubbo:application name="greeting-service-consumer" />
    <!-- 使用zookeeper广播注册中心暴露服务地址 -->
    <dubbo:registry protocol="zookeeper" address="10.1.1.228:2181,10.1.1.225:2181,10.1.1.223:2181" client="zkclient"/>

    <!--<bean id="greetingService" class="com.taomee.bigdata.tj.GreetingServiceImpl"/>

    <dubbo:service protocol="dubbo" interface="com.taomee.bigdata.server.GreetingServer" ref="greetingService"/>-->

    <!--生成远端代理-->
    <dubbo:reference id="greetingServer" interface="com.taomee.bigdata.server.GreetingServer"/>

	
</beans>